<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#fff9fafa" />
            <stroke android:width="1.0px" android:color="#ffe5e6e6" />
            <corners android:topLeftRadius="20.0dip" android:topRightRadius="20.0dip" android:bottomLeftRadius="20.0dip" android:bottomRightRadius="20.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/item_bg_pressed" />
            <stroke android:width="1.0px" android:color="@color/black_40_percent" />
            <corners android:topLeftRadius="20.0dip" android:topRightRadius="20.0dip" android:bottomLeftRadius="20.0dip" android:bottomRightRadius="20.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/white" />
            <stroke android:width="1.0px" android:color="@color/black_40_percent" />
            <corners android:topLeftRadius="20.0dip" android:topRightRadius="20.0dip" android:bottomLeftRadius="20.0dip" android:bottomRightRadius="20.0dip" />
        </shape>
    </item>
</selector>